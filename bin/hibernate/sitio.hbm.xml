<?xml version='1.0'?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="model">
	<class name="Sitio" table="SITIO">
		<id name="idSitio" column="ID_SITIO">
			<generator class="native"/>
		</id>
		
		<!--  <property name="catalogo"/> -->
		<many-to-one name="catalogo" 
					 column="ID_CATALOGO"
					 unique="false"
					 not-null="true"
					 cascade="all"/>		
					 
		<list name="usuarios" table ="USUARIOS_SITIO" cascade="all">
			<key column="idSitio" not-null="true"/>
			<list-index column="line_number"/> <!--  Esto?? -->
			<one-to-many class="Usuario"/>
		</list>
	</class>
</hibernate-mapping>