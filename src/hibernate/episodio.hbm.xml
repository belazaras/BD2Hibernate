<?xml version='1.0'?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="model">
	<class name="Episodio" table="EPISODIO">
		<id name="idEpisodio" column="ID_EPISODIO">
			<generator class="native"/>
		</id>
		<property name="duracion" not-null="true"/>
		<property name="numero" not-null="true"/>
		<property name="titulo" not-null="true"/>

		<many-to-one name="Temporada" column="ID_TEMPORADA" not-null="true" cascade="all"/>	 					
	</class>
</hibernate-mapping>
